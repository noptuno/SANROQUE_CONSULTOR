<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintdis"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backgroudefecto"
    tools:context=".MainDispensador"
    android:keepScreenOn="true">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="548dp"
        android:layout_height="147dp"
        android:layout_marginTop="50dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logocompleto" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="258dp"
        android:layout_height="76dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/btn_fondo_color2"
        android:fontFamily="sans-serif-condensed-light"
        android:gravity="center"
        android:text="TURNO"
        android:textColor="@color/colorAccent"
        android:textSize="50sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/imageView4"
        app:layout_constraintHorizontal_bias="0.518"
        app:layout_constraintStart_toStartOf="@+id/imageView4"
        app:layout_constraintTop_toTopOf="@+id/animation_view2" />

    <Button
        android:id="@+id/btnimprimir"
        android:layout_width="408dp"
        android:layout_height="140dp"
        android:layout_marginBottom="50dp"
        android:background="@drawable/btn_fondo_color5"
        android:fontFamily="sans-serif-condensed-medium"
        android:text="PRESIONE AQUI"
        android:textColor="@color/primaryTextColor"
        android:textSize="50sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="531dp"
        android:layout_height="835dp"
        app:layout_constraintBottom_toTopOf="@+id/btnimprimir"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:srcCompat="@drawable/fondoturnoblanco" />

    <TextView
        android:id="@+id/txtNum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="58"
        android:textAlignment="center"
        android:textColor="@android:color/black"
        android:textSize="250sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView4" />

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/animation_view2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:animateLayoutChanges="true"
        android:scaleType="center"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/imageView4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView4"
        app:layout_constraintVertical_bias="0.485"
        app:lottie_autoPlay="true"
        app:lottie_fileName="loading3.json"
        app:lottie_loop="true" />

</androidx.constraintlayout.widget.ConstraintLayout>